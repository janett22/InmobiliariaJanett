@model InmobiliariaJanett.Models.Contrato

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Dar de baja a un contrato</h1>
<div>
    <hr/>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaFin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inquilino)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Inquilino.Nombre)
            @Html.DisplayFor(model => model.Inquilino.Apellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Inmueble)
        </dt>
        <dd class="col-sm-10">

            @Html.DisplayFor(model => model.Inmueble.Direccion)

        </dd>
    </dl>
    

    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-primary" /> |
        <a asp-action="Index" class="btn btn-secondary">Volver</a>
    </form>
</div>
